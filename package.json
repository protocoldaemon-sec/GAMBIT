    "start:dlq": "node src/workers/dlq.worker.js",
    "start:intelligence": "node src/workers/intelligence.worker.js",
    "start:search-indexer": "node src/workers/search-indexer.worker.js",
    "start:kalshi-stream": "node src/workers/kalshi-stream.worker.js",
